@model FamilleMatierePremiere

<form method="post" asp-action="Save" >
    <input type="hidden" asp-for="Id">
    <div class="form-group">
        <label asp-for="Nom"></label>
        <input class="form-control" asp-for="Nom">
        <span asp-validation-for="Nom"  class="text-warning"></span>

    </div>
    <div class="form-group">
        <label asp-for="DureeOptimaleUtilisation"></label>
        <input class="form-control" asp-for="DureeOptimaleUtilisation">
        <span asp-validation-for="DureeOptimaleUtilisation"  class="text-warning"></span>

    </div>
    <button type="submit" class="btn btn-primary">Enregistrer</button>
    <a asp-action="List" class="btn btn-primary">Annuler</a>
</form>

@section Scripts
{
    <partial name="_ValidationScriptsPartial">
}